<div class="container">
  <div class="my-3">
    <h1>booking requests</h1>
  </div>
  <div class="w-75">
    <% @owner_bookings.each do |booking_as_owner| %>
      <%= render 'bookings/booking_card', booking: booking_as_owner %>
    <% end %>
  </div>
  <div class="my-3">
    <span>
      <h1>your bikes</h1>
      <%= link_to new_bike_path do %>
        <i class="fa-solid fa-plus"></i>
      <% end %>
    </span>
  </div>
  <div class="w-25">
    <% @bikes.each do |bike| %>
      <%= render 'bikes/card', bike: bike %>
    <% end %>
  </div>
</div>
